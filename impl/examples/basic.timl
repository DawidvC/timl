structure Basic = struct

datatype bool = true | false

datatype option 'a =
         NONE
         | SOME of 'a --> 'a option

datatype le {a b : Nat} =
         LE {a <= b} of {b} {a} le
                   
datatype ibool : {Bool} =
         True of {true} ibool
         | False of {false} ibool

end
