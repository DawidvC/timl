datatype {Nat} {Nat} {Bool} 'a rbt =
         Leaf of unit -> {0} {0} {true} 'a rbt
         | Black of {lc : Bool} {rc : Bool} {bh : Nat} {ls : Nat} {rs : Nat}
                                ({ls} {bh} {lc} 'a rbt * 'a * {rs} {bh} {rc} 'a rbt) -> {ls + rs + 1} {bh + 1} {true} 'a rbt
         | Red of {bh : Nat} {ls : Nat} {rs : Nat}
                             ({ls} {bh} {true} 'a rbt * 'a * {rs} {bh} {true} 'a rbt) -> {ls + rs + 1} {bh} {false} 'a rbt

